container_commands:
   12_cache:
      command: "php artisan cache:clear"
      cwd: "/var/app/ondeck"
   13_optimize:
      command: "php artisan optimize --force"
      cwd: "/var/app/ondeck"
   14_permissions:
      # fix permissions on the Laravel app folder
      command: "chmod 777 -R /var/app/ondeck"
      cwd: "/var/app/ondeck"
